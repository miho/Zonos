
{
  "effect_presets": {
    "train_station_pa": {
        "effects": [

          /* 1 ----- steep band-pass 150–5500 Hz */
          { "name": "high_pass_filter",
            "parameters": { "cutoff_hz": 150 } },

          { "name": "low_pass_filter",
            "parameters": { "cutoff_hz": 5500 } },

